{
    "name": "product-discounter/product-discounter",
    "description": "Simple product discounter",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/davidecarbone/php-mongo-migrator"
        }
    ],
    "require": {
        "php": "^7.1",
        "slim/slim": "3.*",
        "oscarotero/psr7-middlewares": "*",
        "gilbitron/php-simplecache": "~1.4",
        "firebase/php-jwt": "^5.0",
        "sokil/php-mongo-migrator": "dev-master",
        "alcaeus/mongo-php-adapter": "^1.1",
        "guzzlehttp/guzzle": "~6.0",
        "ext-mongodb": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Carbone",
            "email": "carboneda@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ProductDiscounter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
